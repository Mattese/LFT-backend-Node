enum Sex {
  MALE
  FEMALE
}

model User {
  id          String    @id @default(cuid()) @db.VarChar(25)
  firstName   String    @db.VarChar(50)
  lastName    String    @db.VarChar(50)
  dateOfBirth DateTime  @db.Date
  nickName    String?   @db.VarChar(30)
  sex         Sex
  memberships GroupMember[]

  @@map("users")
}